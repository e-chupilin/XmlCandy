<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://www.example.com"
	targetNamespace="http://www.example.com" xmlns:cns="http://www.example.com/candy"
	xmlns:ins="http://www.example.com/ingredient" elementFormDefault="qualified">
	<xs:import namespace="http://www.example.com/candy"
		schemaLocation="Candy.xsd" />

	<xs:element name="candys">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="candy" type="cns:candyType" minOccurs="1"
					maxOccurs="unbounded" />
			</xs:sequence>
		</xs:complexType>
	</xs:element>

</xs:schema>